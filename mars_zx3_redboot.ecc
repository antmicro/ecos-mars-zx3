cdl_option CYGSEM_REDBOOT_FIS_CRC_CHECK {
    # This option is not active
    # The parent CYGPKG_REDBOOT_FLASH is not active

    # Flavor: bool
    # No user value, uncomment the following line to provide one.
    # user_value 1
    # value_source default
    # Default value: 1
};

