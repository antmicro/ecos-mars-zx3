    # component CYGDAT_REDBOOT_DEFAULT_GATEWAY_IP_ADDR
    #     DefaultValue: CYGSEM_REDBOOT_FLASH_CONFIG ? 0 :  "0, 0, 0, 0" 
    # component CYGDAT_REDBOOT_DEFAULT_IP_ADDR_MASK
    #     DefaultValue: CYGSEM_REDBOOT_FLASH_CONFIG ? 0 :  "255, 255, 255, 0" 
