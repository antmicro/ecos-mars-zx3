# ---- toplevel --------------------------------------------------------
# This section defines the toplevel configuration object. The only
# values that can be changed are the name of the configuration and
# the description field. It is not possible to modify the target,
# the template or the set of packages simply by editing the lines
# below because these changes have wide-ranging effects. Instead
# the appropriate tools should be used to make such modifications.

cdl_configuration eCos {
    description "" ;

    # These fields should not be modified.
    hardware    mars_zx3 ;
    template    redboot ;
    package -hardware CYGPKG_HAL_ARM current ;
    package -hardware CYGPKG_HAL_ARM_XC7Z current ;
    package -hardware CYGPKG_HAL_ARM_XC7Z020 current ;
    package -hardware CYGPKG_HAL_ARM_MARS_ZX3 current ;
    package -hardware CYGPKG_IO_SPI current ;
    package -hardware CYGPKG_IO_I2C current ;
    package -hardware CYGPKG_DEVS_ETH_ARM_XC7Z current ;
    package -hardware CYGPKG_DEVS_ETH_PHY current ;
    package -hardware CYGPKG_DEVS_I2C_ARM_XC7Z current ;
    package -hardware CYGPKG_IO_ETH_DRIVERS current ;
    package -hardware CYGPKG_REDBOOT current ;
    package -hardware CYGPKG_IO_SERIAL_ARM_XC7Z current ;
    package -hardware CYGPKG_DEVS_QSPI_ARM_XC7Z current ;
    package -hardware CYGPKG_DEVS_FLASH_SPI_M25PXX current ;
    package -template CYGPKG_HAL current ;
    package -template CYGPKG_INFRA current ;
    package -template CYGPKG_ISOINFRA current ;
    package -template CYGPKG_LIBC_STRING current ;
    package -template CYGPKG_CRC current ;
    package -template CYGPKG_ERROR current ;
    package CYGPKG_IO current ;
    package CYGPKG_MEMALLOC current ;
    package CYGPKG_LIBC_I18N current ;
};

